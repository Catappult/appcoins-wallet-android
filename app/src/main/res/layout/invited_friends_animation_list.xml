<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    >
  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/icon_1_2_guideline"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      app:layout_constraintGuide_percent=".215"
      />
  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/icon_2_3_guideline"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      app:layout_constraintGuide_percent=".43"
      />
  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/icon_3_4_guideline"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      app:layout_constraintGuide_percent=".645"
      />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/icon_4_5_guideline"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      app:layout_constraintGuide_percent=".86"
      />

  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/friend_animation_1"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintWidth_default="percent"
      app:layout_constraintWidth_percent=".139"
      app:lottie_autoPlay="true"
      app:lottie_enableMergePathsForKitKatAndAbove="true"
      app:lottie_loop="true"
      app:lottie_rawRes="@raw/not_invited_user_animation"
      />
  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/friend_animation_2"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      app:layout_constraintStart_toEndOf="@id/icon_1_2_guideline"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintWidth_default="percent"
      app:layout_constraintWidth_percent=".139"
      app:lottie_autoPlay="true"
      app:lottie_enableMergePathsForKitKatAndAbove="true"
      app:lottie_loop="true"
      app:lottie_rawRes="@raw/not_invited_user_animation"
      />
  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/friend_animation_3"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      app:layout_constraintStart_toEndOf="@id/icon_2_3_guideline"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintWidth_default="percent"
      app:layout_constraintWidth_percent=".139"
      app:lottie_autoPlay="true"
      app:lottie_enableMergePathsForKitKatAndAbove="true"
      app:lottie_loop="true"
      app:lottie_rawRes="@raw/not_invited_user_animation"
      />
  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/friend_animation_4"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      app:layout_constraintStart_toEndOf="@id/icon_3_4_guideline"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintWidth_default="percent"
      app:layout_constraintWidth_percent=".139"
      app:lottie_autoPlay="true"
      app:lottie_enableMergePathsForKitKatAndAbove="true"
      app:lottie_loop="true"
      app:lottie_rawRes="@raw/not_invited_user_animation"
      />
  <com.airbnb.lottie.LottieAnimationView
      android:id="@+id/friend_animation_5"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      app:layout_constraintStart_toEndOf="@id/icon_4_5_guideline"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintWidth_default="percent"
      app:layout_constraintWidth_percent=".139"
      app:lottie_autoPlay="true"
      app:lottie_enableMergePathsForKitKatAndAbove="true"
      app:lottie_loop="true"
      app:lottie_rawRes="@raw/not_invited_user_animation"
      />

</androidx.constraintlayout.widget.ConstraintLayout>