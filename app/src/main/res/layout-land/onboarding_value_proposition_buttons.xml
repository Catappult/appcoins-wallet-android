<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/onboarding_value_propositions_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    android:layout_marginStart="@dimen/onboarding_padding"
    android:layout_marginEnd="@dimen/onboarding_padding"
    >

  <android.widget.Button
      android:id="@+id/onboarding_back_button"
      style="@style/ButtonStyle.BackButton"
      android:layout_width="50dp"
      android:layout_height="@dimen/onboarding_button_height"
      android:layout_marginEnd="30dp"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@id/onboarding_get_started_button"
      />

  <android.widget.Button
      android:id="@+id/onboarding_get_started_button"
      style="@style/ButtonStyle.Onboarding"
      android:layout_width="207dp"
      android:layout_height="@dimen/onboarding_button_height"
      android:layout_marginStart="27dp"
      android:text="@string/get_started"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"

      />
</androidx.constraintlayout.widget.ConstraintLayout>
